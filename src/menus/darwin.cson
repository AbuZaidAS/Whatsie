      { label: 'Show All', role: 'unhide' }
      { type: 'separator' }
      { label: 'Quit', accelerator: 'Command+Q', command: 'application:quit' }
    ]
  }

  {
    label: 'Edit'
    submenu: [
      { label: 'Undo', accelerator: 'Command+Z', role: 'undo' }
      { label: 'Redo', accelerator: 'Shift+Command+Z', role: 'redo' }
      { type: 'separator' }
      { label: 'Cut', accelerator: 'Command+X', role: 'cut' }
      { label: 'Copy', accelerator: 'Command+C', role: 'copy' }
      { label: 'Paste', accelerator: 'Command+V', role: 'paste' }
      { label: 'Select All', accelerator: 'Command+A', role: 'selectall' }
    ]
  }

  {
    label: 'View'
    submenu: [
      { label: 'Reload', accelerator: 'Command+R', command: 'window:reload' }
      { label: 'Reset Position & Size', accelerator: 'Command+Alt+R', command: 'window:reset' }

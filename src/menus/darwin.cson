  {
    label: 'Theme'
    submenu: [
      { type: 'radio', label: 'Default', command: 'application:update-theme', theme: 'default' }
      { type: 'radio', label: 'Grey', command: 'application:update-theme', theme: 'grey' }
      { type: 'radio', label: 'Pure', command: 'application:update-theme', theme: 'pure' }
      { type: 'radio', label: 'Dark', command: 'application:update-theme', theme: 'dark' }
